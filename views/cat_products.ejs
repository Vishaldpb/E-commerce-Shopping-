<% include _layouts/header %>

<div class="row products">

    <% products.forEach((p)=>{%>
        <div class="col-xs-12 col-md-4">
            <a href="/products/list/<%= p.category %>/<%= p.slug %>" class="pa">
            <img src="/product_images/<%= p.id %>/<%= p.image %>" alt="" class="pimage">
            </a>
            <h3><%= p.title %></h3>
            <h4> $<%= parseFloat(p.price).toFixed(2) %></h4>
            <a href="/products/list/<%= p.category %>/<%= p.slug %>">View Details</a>
        </div>
    <% }) %>

</div>


<% include _layouts/footer %>
